{
  "name": "ee88-agent-hub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js"
  },
  "nodemonConfig": {
    "watch": [
      "*.js",
      "config/",
      "services/",
      "routes/",
      "utils/"
    ],
    "ignore": [
      "logs/"
    ],
    "ext": "js,json",
    "delay": "500"
  },
  "dependencies": {
