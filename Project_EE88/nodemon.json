{
  "watch": ["server/"],
  "ext": "js,json,sql",
  "ignore": [
    "node_modules/",
    "dist/",
    "data/",
    "logs/",
    "*.log",
    "*.db",
    "*.db-journal",
    "*.db-shm",
    "*.db-wal"
  ],
  "exec": "node server/server.js",
  "delay": "500",
  "env": {
    "NODE_ENV": "development"
  }
}
